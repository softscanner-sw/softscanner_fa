{
  "name": "@softscanner/frontend-analyzer",
  "version": "0.1.0",
  "description": "Phase 1 static extractor: derives a deterministic navigationâ€“interaction multigraph from frontend source code.",
  "type": "module",
  "main": "./dist/models/index.js",
  "types": "./dist/models/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/models/index.js",
      "types": "./dist/models/index.d.ts"
    },
    "./models": {
      "import": "./dist/models/index.js",
      "types": "./dist/models/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "build:watch": "tsc --project tsconfig.build.json --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "clean": "rimraf dist",
    "phase1": "tsx src/cli.ts",
    "verify:determinism": "node scripts/verify-determinism.mjs"
  },
  "keywords": [
    "static-analysis",
    "angular",
    "navigation-graph",
    "frontend",
    "AST",
    "multigraph",
    "typescript"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@angular/compiler": "^18.0.0",
    "ts-morph": "^23.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.0",
    "ts-jest": "^29.2.0",
    "tsx": "^4.0.0",
    "typescript": "^5.5.0"
  }
}
